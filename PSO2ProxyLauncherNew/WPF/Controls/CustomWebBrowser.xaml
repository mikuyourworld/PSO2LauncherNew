<UserControl x:Class="PSO2ProxyLauncherNew.WPF.Controls.CustomWebBrowser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PSO2ProxyLauncherNew.WPF.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <WebBrowser Grid.Column="2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                Hyperlink.RequestNavigate="WebBrowser_Hyperlink_RequestNavigate"
                Navigating="WebBrowser_Navigating"
                x:Name="mainWebBrowser"/>
</UserControl>
